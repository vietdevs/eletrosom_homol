<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_form_template_handle"/>
    <body>
        <referenceBlock name="customer_address_edit">
            <action method="setTemplate"  ifconfig="autocompletezipcode/geral/enabled">
                <argument name="template" xsi:type="string">MestreMage_AutoCompleteZipCode::/mestremage/customer/address/edit.phtml</argument>
            </action>
        </referenceBlock>
    </body>
</page>
